{
  "manifest_version": 2,

  "name": "HTML Validator",
  "description": "This extension validates HTML using Nu Html Checker service.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
